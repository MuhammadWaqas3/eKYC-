# eKYC Backend Environment Variables

# LLM API (Required for chat functionality)
GROQ_API_KEY=your-groq-api-key-here

# Database (SQLite by default - no setup needed)
DATABASE_URL=sqlite:///./ekyc_db.sqlite

# JWT Secret (change in production)
JWT_SECRET_KEY=your-secret-key-change-this-in-production



# Application Settings
APP_NAME=Avanza Solutions eKYC
API_HOST=0.0.0.0
API_PORT=8000
FRONTEND_URL=http://localhost:3000

# Security
ALLOWED_ORIGINS=http://localhost:3000,http://process.env.NEXT_PUBLIC_API_URL

Logging
LOG_LEVEL=INFO
